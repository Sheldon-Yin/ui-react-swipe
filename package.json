  },
  "homepage": "https://github.com/JeromeLin/ui-react-swipe",
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "classnames": "2.1.3"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "autoprefixer-loader": "^3.1.0",
    "babel-loader": "^5.3.2",
