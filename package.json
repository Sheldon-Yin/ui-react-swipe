    "build:umd": "webpack src/Swipe.js dist/swipe.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/Swipe.js dist/swipe.min.js --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min"
